{
  "name": "lab-system",
  "version": "2.0.0",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/server.js --ignore client/ --ignore Utilities/",
    "dev:client": "npm --prefix client run dev",
    "start": "node server/server.js",
    "build:client": "npm --prefix client run build",
    "install:client": "npm --prefix client install",
    "postinstall": "npm run install:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaiooFerreiraa/lab_system_backend.git"
  },
  "keywords": [
    "laboratory",
    "management",
    "system"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CaiooFerreiraa/lab_system_backend/issues"
  },
  "homepage": "https://github.com/CaiooFerreiraa/lab_system_backend#readme",
  "description": "Sistema de gerenciamento laboratorial - Backend e Frontend unificados",
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0"
  }
}
