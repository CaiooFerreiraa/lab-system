{
  "name": "lab-system",
  "version": "2.0.0",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/server.js --ignore client/ --ignore Utilities/",
    "dev:client": "npm --prefix client run dev",
    "build": "npm install",
    "install:client": "npm --prefix client install"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "bcryptjs": "^3.0.3",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaiooFerreiraa/lab_system_backend.git"
  },
  "keywords": [
    "laboratory",
    "management",
    "system"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CaiooFerreiraa/lab_system_backend/issues"
  },
  "homepage": "https://github.com/CaiooFerreiraa/lab_system_backend#readme",
  "description": "Sistema de gerenciamento laboratorial - Backend e Frontend unificados"
}
